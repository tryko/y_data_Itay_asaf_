\begin_layout Standard
(1) It is not trivial to show that 
\begin_inset Formula $\int_{\left(a,b\right)}z\,dz=\int_{\left[a,b\right]}z\,dz$
\end_inset

 yet it is intuitive and I assume that it can be done.
\begin_inset Formula 
\begin{align*}
1=\int_{\mathbb{R}}f_{X}\left(x\right)dx & =\int_{-\infty}^{0}f_{X}\left(x\right)dx+\int_{0}^{1}f_{X}\left(x\right)dx\\
 & \ \ +\int_{1}^{2}f_{X}\left(x\right)dx+\int_{2}^{3}f_{X}\left(x\right)dx+\int_{3}^{\infty}f_{X}\left(x\right)dx\\
 & \underset{1}{=}0+\int_{0}^{1}f_{X}\left(x\right)dx+\int_{1}^{2}f_{X}\left(x\right)dx+\int_{2}^{3}f_{X}\left(x\right)dx+0\\
 & =\int_{0}^{1}ax\,dx+\int_{1}^{2}a\,dx+\int_{2}^{3}a\left(3-x\right)\,dx\\
 & =\frac{a}{2}x^{2}\mid_{0}^{1}+ax\mid_{1}^{2}+a\left(3x-\frac{x^{2}}{2}\right)\mid_{2}^{3}\\
 & =\frac{a}{2}+a+a\left(\left(9-\frac{9}{2}\right)-\left(6-\frac{4}{2}\right)\right)=\frac{a}{2}+a+a\left(3-2.5\right)\\
\iff & a=\frac{1}{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
(2) given 
\begin_inset Formula $a=\frac{1}{2}$
\end_inset

 we get 
\begin_inset Formula 
\begin{align*}
f_{X}\left(x\right) & =\begin{cases}
\frac{x}{2} & x\in\left(0,1\right)\\
\frac{1}{2} & x\in\left[1,2\right)\\
\frac{3-x}{2} & x\in\left[2,3\right)\\
0 & o.w
\end{cases}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
EX=\int_{\mathbb{R}}x\cdot f_{X}\left(x\right)\,dx & =\int_{0}^{1}\frac{x^{2}}{2}\,dx+\int_{1}^{2}\frac{x}{2}\,dx+\int_{2}^{3}\frac{3x-x^{2}}{2}\,dx\\
 & =\frac{1}{2}\left[\int_{0}^{1}x^{2}\,dx+\int_{1}^{2}x\,dx+\int_{2}^{3}3x-x^{2}\,dx\right]\\
 & =\frac{1}{2}\left[\frac{1}{3}+\left(\frac{4}{2}-\frac{1}{2}\right)+\left(\left(\frac{27}{2}-\frac{27}{3}\right)-\left(\frac{12}{2}-\frac{8}{3}\right)\right)\right]\\
 & =\frac{1}{2}\left[\frac{1}{3}+\frac{3}{2}+\frac{7}{6}\right]=\frac{1}{2}\left[\frac{2}{6}+\frac{9}{6}+\frac{7}{6}\right]\\
 & =\frac{3}{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
Var\left(X\right) & =E\left(X-EX\right)^{2}\\
 & =EX^{2}-E^{2}X\\
 & =\int_{\mathbb{R}}x^{2}\cdot f_{X}\left(x\right)\,dx-\left(\frac{3}{2}\right)^{2}\\
 & =\left[\int_{0}^{1}\frac{x^{3}}{2}\,dx+\int_{1}^{2}\frac{x^{2}}{2}\,dx+\int_{2}^{3}\frac{3x^{2}-x^{3}}{2}\,dx\right]-\frac{9}{4}\\
 & =\frac{1}{2}\left[\frac{x^{4}}{4}\mid_{0}^{1}+\frac{x^{3}}{3}\mid_{1}^{2}+\left(3\frac{x^{3}}{3}-\frac{x^{4}}{4}\right)\mid_{2}^{3}\right]-\frac{9}{4}\\
 & =\frac{1}{2}\left[\frac{1}{4}+\frac{7}{3}+\left(27-\frac{81}{4}-\left(8-\frac{16}{4}\right)\right)\right]-\frac{9}{4}\\
 & =\frac{1}{2}\left[\frac{3}{12}+\frac{28}{12}+\frac{3}{3}\left(\frac{92}{4}-\frac{81}{4}\right)\right]-\frac{9}{4}\\
 & =\frac{1}{2}\left[\frac{31}{12}+\frac{33}{12}\right]-\frac{27}{12}=\frac{1}{2}\cdot\frac{64}{12}-\frac{27}{12}\\
 & =\frac{5}{12}
\end{align*}

\end_inset

 
\end_layout

