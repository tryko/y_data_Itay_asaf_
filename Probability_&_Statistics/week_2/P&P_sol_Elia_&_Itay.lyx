\begin_layout Problem
Let 
\begin_inset Formula $X_{1},X_{2},...$
\end_inset

 be a sequence of independent and identically distributed random variables
 with distribution 
\begin_inset Formula $Bin\left(48,1/4\right)$
\end_inset

.
 Let 
\begin_inset Formula $\overline{X}=\frac{1}{n}\sum_{i=1}^{n}X_{i}$
\end_inset

.
 Use the central limit theorem to calculate 
\begin_inset Formula $P\left(\overline{X}_{144}>12.75\right)$
\end_inset


\end_layout

\begin_layout Subsection*
Answer: 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\text{P\left(\overline{X}_{144}>12.75\right)} & =1-P\left(\overline{X}_{144}\leq12.75\right)\\
 & =1-P\left(\sqrt{144}\cdot\frac{\overline{X}_{144}-\mu_{X}}{\sigma_{X}}\leq\sqrt{144}\cdot\frac{12.75-\mu_{X}}{\sigma_{X}}\right)\\
 & \underset{CLT}{=}1-P\left(z\leq12\cdot\frac{12.75-\mu_{X}}{\sigma_{X}}\right)\\
 & =1-\phi\left(12\cdot\frac{12.75-\mu_{X}}{\sigma_{X}}\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
We need to find 
\begin_inset Formula $\mu_{X},\sigma_{X}$
\end_inset

.
 
\end_layout

\begin_layout Standard
Notice that for 
\begin_inset Formula $X\sim Bin\left(n,p\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\mu_{X} & =EX=E\left[\sum_{i=1}^{n}X_{i}\right]\underset{linearity}{=}\sum_{i=1}^{n}EX_{i}\underset{iid}{=}nEX_{i}\\
 & =n\left[0\cdot p^{0}\left(1-p\right)^{1}+1\cdot p^{1}\left(1-p\right)^{0}\right]=np\\
\implies & \mu_{X}=48\cdot\frac{1}{4}=12\\
Var\left(X\right) & =Var\left(\sum_{i=1}^{n}X_{i}\right)\underset{*}{=}\sum_{i=1}^{n}Var\left(X_{i}\right)\underset{iid}{=}n\cdot Var\left(X_{i}\right)\\
 & =n\cdot\left[E\left[X_{i}-EX_{i}\right]^{2}\right]=n\cdot\left[EX_{i}^{2}-p^{2}\right]\\
 & =n\cdot\left[0^{2}\cdot p^{0}\left(1-p\right)^{1}+1^{2}\cdot p^{1}\left(1-p\right)^{0}-p^{2}\right]\\
 & =n\left[p-p^{2}\right]=np\left(1-p\right)\\
\implies & \sigma_{X}^{2}=48\cdot\frac{1}{4}\cdot\frac{3}{4}=9\implies\sigma_{X}=3
\end{align*}

\end_inset

 *: 
\begin_inset Formula $Cov\left(X_{i},X_{j}\right)=0\ \forall i\neq j$
\end_inset

.
 Thus 
\begin_inset Formula 
\begin{align*}
P\left(\overline{X}_{144}>12.75\right) & =1-\phi\left(12\cdot\frac{12.75-12}{3}\right)\\
 & =1-\phi\left(12\cdot\frac{0.75}{3}\right)=1-\phi\left(3\right)\\
 & =1-0.9987=0.0013
\end{align*}

\end_inset


\end_layout

